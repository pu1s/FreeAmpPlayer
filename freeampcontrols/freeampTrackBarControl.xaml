<UserControl x:Name="freeampTrackBar" x:Class="freeampcontrols.freeampTrackBarControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:freeampcontrols"
             mc:Ignorable="d" 
             d:DesignHeight="14" d:DesignWidth="300">
    <Border Height="14">
        <Slider x:Name="slider" Margin="0" Style="{DynamicResource SimpleSlider}" VerticalAlignment="Center" Value="5"/>

    </Border>
</UserControl>
